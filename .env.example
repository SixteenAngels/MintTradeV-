# Example environment variables for Mint Trade
# Copy to .env (for local dev) and set values or configure CI/EAS secrets accordingly.

# Expo app (public, bundled at build time)
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=

# Firebase Functions base URL (after you deploy Functions)
# Example: https://<region>-<project>.cloudfunctions.net/api
EXPO_PUBLIC_API_BASE=

# Sentry (optional but recommended)
EXPO_PUBLIC_SENTRY_DSN=

# GSE market data API (defaults to https://dev.kwayisi.org/apis/gse)
EXPO_PUBLIC_GSE_API_BASE_URL=https://dev.kwayisi.org/apis/gse

# WalletConnect (planned re-integration; safe to leave empty for now)
EXPO_PUBLIC_WALLETCONNECT_PROJECT_ID=

# Firebase Functions runtime (server-only; set via Firebase secrets)
# Run: firebase functions:secrets:set FINNHUB_API_KEY
FINNHUB_API_KEY=
